{
  "defaultRule": "FORBIDDEN",
  "rules": [
    {
      "from": "Woche6.ExampleArchitecture.benchmark_two.service",
      "to": "Woche6.ExampleArchitecture.benchmark_two.util",
      "type": "ALLOWED",
      "except": [
        {
          "from": "Woche6.ExampleArchitecture.benchmark_two.service.NotificationService",
          "to": "Woche6.ExampleArchitecture.benchmark_two.util.Logger",
          "type": "FORBIDDEN"
        },
        {
          "from": "Woche6.ExampleArchitecture.benchmark_two.service.Router",
          "to": "Woche6.ExampleArchitecture.benchmark_two.util.ConfigLoader",
          "type": "FORBIDDEN"
        }
      ]
    },
    {
      "from": "Woche6.ExampleArchitecture.benchmark_two.domain",
      "to": "Woche6.ExampleArchitecture.benchmark_two.service",
      "type": "FORBIDDEN",
      "except": [
        {
          "from": "Woche6.ExampleArchitecture.benchmark_two.domain.User",
          "to": "Woche6.ExampleArchitecture.benchmark_two.service.NotificationService",
          "type": "ALLOWED"
        },
        {
          "from": "Woche6.ExampleArchitecture.benchmark_two.domain.Order",
          "to": "Woche6.ExampleArchitecture.benchmark_two.service.InvoiceService",
          "type": "ALLOWED"
        },
        {
          "from": "Woche6.ExampleArchitecture.benchmark_two.domain.InventoryItem",
          "to": "Woche6.ExampleArchitecture.benchmark_two.service.InventoryService",
          "type": "ALLOWED"
        }
      ]
    }
  ]
}
